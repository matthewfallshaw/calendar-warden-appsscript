{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "max-line-length": {
      "options": [
        120
      ]
    },
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ],
    "member-access": [true, "no-public"],
    "no-console": false
  },
  "rulesDirectory": []
}
